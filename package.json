    "type": "git",
    "url": "https://github.com/lc-semba-ryuichiro/practice-next-ec.git"
  },
  "license": "MIT",
  "author": "Ryuichiro Semba",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "build-storybook": "storybook build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "fix": "run-s fix:prettier fix:eslint fix:stylelint",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --write .",
    "fix:remark": "remark . --output",
    "fix:stylelint": "stylelint \"**/*.css\" --fix",
    "fix:textlint": "textlint --fix \"**/*.md\"",
    "preinstall": "npx only-allow pnpm",
    "lint": "turbo run lint",
    "lint:browserslist": "browserslist",
    "lint:prettier": "prettier --check .",
    "lint:remark": "remark . --quiet",
    "lint:secretlint": "secretlint \"**/*\"",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:textlint": "textlint \"**/*.md\"",
    "prepare": "lefthook install",
    "storybook": "storybook dev -p 6006",
    "test": "turbo run test",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "typecheck": "turbo run typecheck"
  },
