    "lint:textlint": "textlint \"**/*.md\"",
    "lint:types": "tsc --noEmit",
    "prepare": "lefthook install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit"
